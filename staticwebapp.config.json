{
  "routes": [
    {
      "route": "/.well-known/mta-sts.txt",
      "serve": "/.well-known/mta-sts.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "route": "/.well-known/mta-sts-*.txt",
      "serve": "/.well-known/mta-sts-*.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "route": "/.well-known/*",
      "serve": "/.well-known/*",
      "headers": {
        "Content-Type": "text/plain"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "mimeTypes": {
    ".txt": "text/plain"
  }
}
